# platform :ios, '9.0'

target 'Cycle' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Cycle
  pod 'RxSwift'
  pod 'RxCocoa'

  target 'CycleTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking'
